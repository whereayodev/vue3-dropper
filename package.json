{
  "name": "vue3-dropper",
  "description": "A simple dropdown vue3 library",
  "main": "./dist/drop-down.umd.js",
  "module": "./dist/drop-down.js",
  "types": "./dist/index.d.ts",
  "version": "0.1.0",
  "exports": {
    ".": {
      "import": "./dist/drop-down.esm.js",
      "require": "./dist/drop-down.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "vue",
    "vue3",
    "component",
    "library"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@vueuse/core": "^10.1.2",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.24",
    "sass": "^1.63.2",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@babel/types": "^7.22.5",
    "@rushstack/eslint-patch": "^1.3.1",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.8",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.42.0",
    "eslint-plugin-vue": "^9.11.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "~5.0.4",
    "unplugin-vue-components": "^0.25.0",
    "vite": "^4.3.5",
    "vite-plugin-dts": "^2.3.0",
    "vue-tsc": "^1.6.5"
  }
